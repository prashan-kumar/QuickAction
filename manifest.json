{
  "manifest_version":3,
  "name":"QuickAction",
  "description":"Save your time and get things done faster",
  "version":"1.0.0",
  "icons":{
    "16": "icons/clock-16.png",
    "32": "icons/clock-32.png",
    "48": "icons/clock-48.png",
    "128": "icons/clock-128.png"
  },

  "action": {
    "default_icon": {
      "16": "icons/clock-16.png",
      "32": "icons/clock-32.png",
      "48": "icons/clock-48.png",
      "128": "icons/clock-128.png"
    }
  },
    "permissions": ["storage","contextMenus","alarms","notifications","tabs"],
    "background":{
      "service_worker": "service_worker.js"
    }
  

}